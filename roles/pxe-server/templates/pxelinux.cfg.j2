default coreos
prompt 1
timeout 15

display boot.msg

label coreos
  menu default
  kernel coreos_production_pxe.vmlinuz
  initrd coreos_production_pxe_image.cpio.gz
  append coreos.first_boot=1 coreos.config.url=http://{{ ansible_default_ipv4.address }}/{{ item }}-container-linux-config.ign root=LABEL=ROOT
